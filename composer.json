{
    "name": "sandritsch91/yii2-widget-jsignature",
    "description": "A jSignature widget for Yii2",
    "type": "yii2-extension",
    "keywords": [
        "jSignature",
        "widget",
        "extension",
        "yii2"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Sandritsch91",
        "email": "sandritsch91@gmail.com"
    }],
    "support": {
        "source": "https://github.com/Sandritsch91/yii2-widget-jSignature",
        "issues": "https://github.com/Sandritsch91/yii2-widget-jSignature/issues"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "yiisoft/yii2": "^2.0.50",
        "bower-asset/jsignature": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "sandritsch91\\yii2\\jSignature\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
